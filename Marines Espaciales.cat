    <selectionEntry id="28ff-bd37-a713-b9fb" name="Capitán Cato Sicarius" publicationId="f176-f140-74d8-da7d" page="85" hidden="false" collective="false" import="true" type="unit">
      <constraints>
        <constraint field="selections" scope="roster" value="1.0" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" id="ab60-eda4-907f-faec" type="max"/>
